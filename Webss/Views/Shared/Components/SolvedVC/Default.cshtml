@model IEnumerable<MockTest>

<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h2 class="m-0 font-weight-bold text-primary">Solved Mock Tests</h2>
    </div>

    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
            <tr>
                <th>MockTestID</th>
                <th>MockTestName</th>
                <th>MockTestSubject</th>

            </tr>
        </thead>
        <tbody>
            @foreach (var temp in Model)
            {
                    <tr>
                        <td>@temp.MockTestID</td>
                        <td>@temp.MockTestName</td>
                        <td>@temp.MockTestSubject.SubjectTitle</td>

                    </tr>
            }
        </tbody>
    </table>
</div>
