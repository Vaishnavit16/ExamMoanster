


@model PackageUPIPayment

@{
    ViewData["Title"] = "ChequePayment";
}




<hr />

<div style="background-color:white; padding: 20px;">
    <!-- White background here -->
    <div class="row">
        <div class="col-md-4">
            <form asp-action="ChequePayment">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="form-group">
                    <label asp-for="PackagePurchasePaymentID" class="control-label"></label>
                    <input type="hidden" asp-for="PackagePurchasePaymentID" value="@ViewBag.id" class="form-control" />
                    <span asp-validation-for="PackagePurchasePaymentID" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <label asp-for="BankName" class="control-label"></label>
                    <input asp-for="BankName" class="form-control" />
                    <span asp-validation-for="BankName" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <input type="submit" value="Create" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>
