@model CompanyRegisterDto

<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>SiteAdmin 2 - Login</title>
    <script src="~/jquery/jquery.js"></script>
    <script src="~/jquery-validate/jquery.validate.js"></script>
    <script src="~/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>

    <!-- Custom fonts for this template-->
    <link href="~/backendassets/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/backendassets/css/sb-admin-2.min.css" rel="stylesheet">

</head>

<body class="bg-gradient-primary">

    <div class="container">

        <!-- Outer Row -->
        <div class="row justify-content-center">

            <div class="col-xl-10 col-lg-12 col-md-9">

                <div class="card o-hidden border-0 shadow-lg my-5">
                    <div class="card-body p-0">
                        <!-- Nested Row within Card Body -->
                        <div class="row">
                            <div class="col-lg-6 d-none d-lg-block bg-login-image"></div>
                            <div class="col-lg-6">
                                <div class="p-5">
                                    <div class="text-center">
                                        <h1 class="h4 text-gray-900 mb-4">Welcome!</h1>
                                    </div>
                                    <div class="text-centre small text-danger " asp-validation-summary="ModelOnly"> </div>
                                    <form asp-action="Register" method="post">
                                        <div asp-validation-summary="ModelOnly"> </div>
                                        <div class="form-group">
                                            <b>  <label asp-for="CompanyName">CompanyName </label></b>
                                            <input type="CompanyName" class="form-control form-control-user" asp-for="CompanyName" placeholder="CompanyName" />
                                            <span class="text-danger" asp-validation-for="CompanyName"> </span>
                                       </div>

                                        <div class="form-group">
                                            <b>  <label asp-for="Address">Address </label></b>
                                            <input type="Address" class="form-control form-control-user" asp-for="Address" placeholder="Address" />
                                            <span class="text-danger" asp-validation-for="Address"> </span>
                                        </div>

                                        <div class="form-group">
                                            <b>  <label asp-for="EmailID">EmailID </label></b>
                                            <input type="EmailID" class="form-control form-control-user" asp-for="EmailID" placeholder="EmailID" />
                                            <span class="text-danger" class="text-danger" asp-validation-for="EmailID"> </span>
                                        </div>

                                        <div class="form-group">
                                            <b><label asp-for="ContactPersonName">ContactPersonName </label></b>
                                            <input type="ContactPersonName" class="form-control form-control-user" asp-for="ContactPersonName" placeholder="ContactPersonName" />
                                            <span class="text-danger" asp-validation-for="ContactPersonName"> </span>
                                        </div>


                                        <div class="form-group">
                                            <b>  <label asp-for="ContactNo">ContactNo </label></b>
                                            <input type="ContactNo" class="form-control form-control-user" asp-for="ContactNo" placeholder="ContactNo" />
                                            <span class="text-danger" asp-validation-for="ContactNo"> </span>
                                        </div>


                                        <div class="form-group">
                                            <b> <label asp-for="CompanyDesc">CompanyDesc </label></b>
                                            <input type="CompanyDesc" class="form-control form-control-user" asp-for="CompanyDesc" placeholder="CompanyDesc" />
                                            <span class="text-danger" asp-validation-for="CompanyDesc"> </span>
                                        </div>



                                        <div class="form-group">
                                            <b> <label asp-for="Password">Password </label></b>
                                            <input type="Password" class="form-control form-control-user" asp-for="Password" placeholder="Password" />
                                            <span class="text-danger" asp-validation-for="Password"> </span>
                                        </div>


                                        <div class="form-group">
                                            <b>   <label asp-for="RegistrationDate">RegistrationDate </label></b>
                                            <input type="RegistrationDate" class="form-control form-control-user" asp-for="RegistrationDate" placeholder="RegistrationDate" />
                                            <span class="text-danger" asp-validation-for="RegistrationDate"> </span>
                                        </div>

                                        <div class="form-group">
                                            <b><label asp-for="ActiveFlag">ActiveFlag </label></b>
                                            <input type="ActiveFlag" class="form-control form-control-user" asp-for="ActiveFlag" placeholder="ActiveFlag" />
                                            <span class="text-danger" asp-validation-for="ActiveFlag"> </span>
                                        </div>

                                       <div class="mt-3">
                                            <button class="btn btn-primary btn-user btn-block" type="submit">
                                                Save
                                            </button>
                                        </div>

                                    </form>
                                    <hr>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>

        </div>

    </div>

    <!-- Bootstrap core JavaScript-->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="js/sb-admin-2.min.js"></script>

</body>

</html>