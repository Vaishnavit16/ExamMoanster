 @model MockTestQuestionDetailsss



<div class="col-5">
    <div class="card">
        <div class="card-body">
            <div class="card-header py-3">
                <h4 class="m-0 font-weight-bold text-primary">MockTestQuestion Details</h4>
            </div>


            <div class="container">
                <div class="row">
                    <div class="col-md-12">


                        <table class=" table table-bordered">



                            <tr>
                                <td> Question</td>
                                <td> @Model.Questions</td>
                            </tr>





                        </table>

                        <div class="card-header py-3">
                            <h4 class="m-0 font-weight-bold text-primary">MockTestQuestionOption Details</h4>
                        </div>

                        <div class="container">
                            <div class="row">
                                <div class="col-md-12">
                                    <table class=" table table-bordered">



                                        <thead>
                                            <tr>
                                                <td> Option1</td>
                                                <td> Option2</td>
                                                <td> Option3</td>
                                                <td> Option4</td>

                                            </tr>
                                        </thead>




                                        <tr>
                                            <td> @Model.Option1</td>
                                            <td> @Model.Option2</td>
                                            <td> @Model.Option3</td>
                                            <td> @Model.Option4</td>

                                        </tr>

                                    </table>


                                </div>
                            </div>
                        </div>



                        <div class="card-header py-3">
                            <h4 class="m-0 font-weight-bold text-primary">MockTestQuestionAnswer</h4>
                        </div>

                        <div class="container">
                            <div class="row">
                                <div class="col-md-12">
                                    <table class="table table-bordered">
                                        <tr>
                                            <td>Correct Answer</td>
                                            <td>@Model.CorrectAnswer</td>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>










                    </div>
                </div>
            </div>


            <div class="mt-3">
                <a asp-action="Index" asp-controller="MockTestQuestion" class="btn btn-primary"> Back To Index</a>
            </div>
        </div>
    </div>
</div>


