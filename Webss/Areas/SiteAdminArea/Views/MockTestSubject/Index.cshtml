@model IEnumerable<MockTestSubject>

<html>
<head>
    <title> </title>
</head>
<body>
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h2 class="m-0 font-weight-bold text-primary">MockTestSubject List</h2>
        </div>


        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="mt-3">
                        <a asp-action="Create" asp-controller="MockTestSubject" class="btn btn-primary">  Create QuestionBank</a>
                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                <thead>
                                    <tr>
                                        <th>MockTestSubjectID</th>
                                        <th>SubjectTitle </th>
                                        <th>SubjectDesc </th>
                                        <th>MockTestCategoryName</th>


                                    </tr>
                                </thead>


                                <tbody>
                                    @foreach (var temp in Model)
                                    {
                                        <tr>
                                            <td> @temp.MockTestSubjectID</td>
                                            <td> @temp.SubjectTitle</td>
                                            <td> @temp.SubjectDesc.</td>
                                            <td> @temp.MockTestCategory.MockTestCategoryName</td>

                                            <td>

                                                <a asp-action="Edit" asp-route-id="@temp.MockTestSubjectID" class="btn btn-primary"> Edit </a>
                                                 <a asp-action="Delete" asp-route-id="@temp.MockTestSubjectID" class="btn btn-primary"> Delete </a> 
                                            </td>
                                        </tr>
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


</body>
</html>